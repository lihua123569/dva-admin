{
  "entry": "src/index.js",
   "disableCSSModules": false,
  "less": false,
  "publicPath": "/",
  "autoprefixer": null,
   "proxy": {
      "/api": {
        "target": "http://jsonplaceholder.typicode.com/",
        "changeOrigin": true,
        "pathRewrite": { "^/api" : "" }
      }
    },
     "extraBabelPlugins": [
        "transform-runtime",
